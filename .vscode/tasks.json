{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "go-build-file",
            "type": "process",
            "command": "/usr/local/go/bin/go",
            "args": [
                "build"
            ],
            "options": {
                "cwd": "${relativeFileDirname}",
    
            },
            "problemMatcher": ["$go"],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}